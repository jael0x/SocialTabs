{
    "manifest_version": 3,
    "name": "Social Tabs",
    "version": "1.0",
    "description": "Social Tabs allows to open your social networks in new tabs | Social Tabs permite abrir tus redes sociales en nuevas pesta√±as",
    "author": "jaelarmasm",
    "icons": {
        "16": "src/logo/logo16.png",
        "32": "src/logo/logo32.png",
        "48": "src/logo/logo48.png",
        "128": "src/logo/logo128.png"
    },
    "action": {
        "default_title": "Social Tabs",
        "default_popup": "src/popup.html",
        "default_icon": {
            "16": "src/logo/logo16.png",
            "32": "src/logo/logo32.png",
            "48": "src/logo/logo48.png",
            "128": "src/logo/logo128.png"
        }
    },
    "homepage_url": "https://jaelarmasm.com",
    "options_page": "src/options.html",
    "permissions": [
        "storage"
    ]
}