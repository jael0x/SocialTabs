{
    "name": "Open Social Media Webs",
    "author": "jaelarmasm",
    "version": "1.2",
    "manifest_version": 2,
    "description": "Abrir las páginas webs de las redes sociales más conocidas en una nueva pestaña",
    "icons": {
        "16": "src/icons/logo16.png",
        "32": "src/icons/logo32.png",
        "48": "src/icons/logo48.png",
        "128": "src/icons/logo128.png"
    },
    "browser_action": {
        "default_title": "Open Social Media Webs",
        "default_popup": "src/popup.html",
        "default_icon": {
            "16": "src/icons/logo16.png",
            "32": "src/icons/logo32.png",
            "48": "src/icons/logo48.png",
            "128": "src/icons/logo128.png"
        }
    },
    "permissions": ["storage"],
    "options_page": "src/options.html"
}