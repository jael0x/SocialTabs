{
    "name": "SocialTabs: abrir redes sociales en una nueva pestaña",
    "author": "jaelarmasm",
    "version": "1.0",
    "manifest_version": 2,
    "description": "SocialTabs es una extensión para abrir las redes sociales más utilizadas en una nueva pestaña.",
    "icons": {
        "16": "src/logo/logo16.png",
        "32": "src/logo/logo32.png",
        "48": "src/logo/logo48.png",
        "128": "src/logo/logo128.png"
    },
    "browser_action": {
        "default_title": "SocialTabs: abrir redes sociales en una nueva pestaña",
        "default_popup": "src/popup.html",
        "default_icon": {
            "16": "src/logo/logo16.png",
            "32": "src/logo/logo32.png",
            "48": "src/logo/logo48.png",
            "128": "src/logo/logo128.png"
        }
    },
    "permissions": [
        "storage"
    ],
    "options_page": "src/options.html",
    "homepage_url": "http://jaelarmasm.com"
}